{
    "version": "0.1.0",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start:node": "tsdx watch",
        "build:node": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "build:cjs": "tsc && webpack &&  cp -r src/styles.css ./dist && cp -r dist/maxtap_public.js examples/classic_web",
        "react-test": "cd examples/reactjs && npm start"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "maxtap_public",
    "author": "MAXTAP",
    "module": "dist/maxtap_node_module.esm.js",
    "size-limit": [
        {
            "path": "dist/maxtap_node_module.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/maxtap_node_module.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.5",
        "husky": "^7.0.4",
        "size-limit": "^7.0.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^3.9.10"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/maxtap/javascript-plugin"
    },
    "dependencies": {
        "@babel/core": "^7.16.7",
        "@babel/preset-env": "^7.16.7",
        "@babel/runtime": "^7.16.7",
        "babel-loader": "^8.2.3",
        "regenerator-runtime": "^0.13.9",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1"
    }
}